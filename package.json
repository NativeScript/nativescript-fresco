{
  "name": "nativescript-fresco",
  "version": "3.0.3",
  "description": "This is a NativeScript plugin wrapping the popular Fresco library for managing images on Android.",
  "main": "nativescript-fresco",
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/nativescript-fresco"
  },
  "bugs": {
    "url": "https://github.com/NativeScript/nativescript-fresco/issues"
  },
  "files": [
    "*.js*",
    "platforms",
    "*.d.ts",
    "angular/*.js*",
    "angular/*.d.ts",
    "!tsconfig.json"
  ],
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "devDependencies": {
    "@angular/core": "~4.1.0",
    "@angular/compiler": "~4.1.0",
    "@angular/compiler-cli": "~4.1.0",
    "nativescript-angular": "next",
    "rxjs": "^5.0.1",
    "tns-core-modules": "next",
    "tns-platform-declarations": "next",
    "typescript": "^2.2.2"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.0.0 || >3.2.0-"
  },
  "scripts": {
    "prepublish": "ngc"
  },
  "keywords": [
    "fresco",
    "android",
    "nativescript"
  ],
  "author": "Deyan Ginev <Deyan.Ginev@telerik.com> & Vladimir Amiorkov <Vladimir.Amiorkov@telerik.com> ",
  "contributors": [
    {
      "name": "Deyan Ginev",
      "email": "Deyan.Ginev@telerik.com"
    },
    {
      "name": "Vladimir Amiorkov",
      "email": "Vladimir.Amiorkov@telerik.com"
    }
  ],
  "license": "ISC"
}
